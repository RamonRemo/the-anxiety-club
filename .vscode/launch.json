    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Brave",
            "request": "launch",
            "type": "chrome",
            "runtimeExecutable": "/usr/bin/brave-browser",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "the-anxiety-club (web)",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome"
        },
        {
            "name": "the-anxiety-club",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "the-anxiety-club (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "the-anxiety-club (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}